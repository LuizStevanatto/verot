{
  "compilerOptions": {
    "target": "es2017",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,            
    "strict": true,
    "allowJs": true,
    "skipLibCheck": true,
    "strictPropertyInitialization": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "strictNullChecks": false                        
  },
    "include": [
      "src/**/*.ts"
    ]
  }